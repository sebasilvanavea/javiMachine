{
  "name": "massive-data-generator",
  "version": "1.0.0",
  "description": "Script para generar datos masivos en Firebase",
  "main": "generate-data.js",
  "scripts": {
    "generate": "node generate-data.js",
    "generate-users": "node generate-data.js --type=users --count=1000",
    "generate-services": "node generate-data.js --type=services --count=2000",
    "generate-forms": "node generate-data.js --type=form21 --count=500",
    "clear-test-data": "node generate-data.js --clear",
    "install-deps": "npm install firebase-admin dotenv"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "dotenv": "^16.0.0"
  },
  "keywords": ["firebase", "data-generator", "testing"],
  "author": "Sistema Contable",
  "license": "MIT"
}
